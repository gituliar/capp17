Cell[CellGroupData[{

Cell["1. Physical Setup", "Chapter"],

Cell[TextData[{
 "We consider the process ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["e", "+"], "(", 
      SubscriptBox["q", "1"], ")"}], "+", 
     RowBox[{
      SuperscriptBox["e", "-"], " ", 
      RowBox[{"(", 
       SubscriptBox["q", "2"], ")"}]}]}], " ", "\[Rule]", " ", 
    RowBox[{
     RowBox[{"q", " ", 
      RowBox[{"(", 
       SubscriptBox["p", "1"], ")"}]}], "+", " ", 
     RowBox[{"qbar", " ", 
      RowBox[{"(", 
       SubscriptBox["p", "2"], ")"}]}], "+", 
     RowBox[{"g", " ", 
      RowBox[{"(", 
       SubscriptBox["p", "3"], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nThe criss-section for this process is given by"
}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"M2", "=", 
   RowBox[{"x", 
    FractionBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"sp", "[", 
        RowBox[{"p1", ",", "q1"}], "]"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"sp", "[", 
        RowBox[{"p1", ",", "q2"}], "]"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"sp", "[", 
        RowBox[{"p2", ",", "q1"}], "]"}], "2"], "+", 
      SuperscriptBox[
       RowBox[{"sp", "[", 
        RowBox[{"p2", ",", "q2"}], "]"}], "2"]}], 
     RowBox[{
      RowBox[{"sp", "[", 
       RowBox[{"q1", ",", "q2"}], "]"}], 
      RowBox[{"sp", "[", 
       RowBox[{"p1", ",", "p3"}], "]"}], 
      RowBox[{"sp", "[", 
       RowBox[{"p2", ",", "p3"}], "]"}]}]]}]}], ";"}]], "Input"]
}, Closed]],

