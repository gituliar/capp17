Cell[CellGroupData[{

Cell["Partial Fractioning", "Subchapter"],

Cell[TextData[{
 "Taking into account that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     RowBox[{
      RowBox[{"sp", "[", 
       RowBox[{"p1", ",", "p3"}], "]"}], 
      RowBox[{"sp", "[", 
       RowBox[{"p2", ",", "p3"}], "]"}]}]], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "x"}]], 
     RowBox[{"(", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"sp", "[", 
         RowBox[{
          SubscriptBox["p", "1"], ",", 
          SubscriptBox["p", "3"]}], "]"}]], "+", 
       FractionBox["1", 
        RowBox[{"sp", "[", 
         RowBox[{
          SubscriptBox["p", "2"], ",", 
          SubscriptBox["p", "3"]}], "]"}]]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    FractionBox[
     RowBox[{"sp", "[", 
      RowBox[{
       RowBox[{
        SubscriptBox["q", "1"], "+", 
        SubscriptBox["q", "2"]}], ",", 
       SubscriptBox["p", "3"]}], "]"}], 
     RowBox[{"sp", "[", 
      RowBox[{
       SubscriptBox["q", "1"], ",", 
       SubscriptBox["q", "2"]}], "]"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "we split M2=M2a+M2b like this"
}], "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"M2a", "=", 
   FractionBox[
    RowBox[{
     SuperscriptBox[
      RowBox[{"sp", "[", 
       RowBox[{"p1", ",", "q1"}], "]"}], "2"], "+", 
     SuperscriptBox[
      RowBox[{"sp", "[", 
       RowBox[{"p1", ",", "q2"}], "]"}], "2"], "+", 
     SuperscriptBox[
      RowBox[{"sp", "[", 
       RowBox[{"p2", ",", "q1"}], "]"}], "2"], "+", 
     SuperscriptBox[
      RowBox[{"sp", "[", 
       RowBox[{"p2", ",", "q2"}], "]"}], "2"]}], 
    RowBox[{"2", "x", " ", 
     RowBox[{"sp", "[", 
      RowBox[{"q1", ",", "q2"}], "]"}], 
     RowBox[{"sp", "[", 
