Cell["ODE Solver", "Section"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"$SolveODE", "[", 
    RowBox[{"m_", ",", "x_", ",", "ep_", ",", "n_", ",", "c_"}], "]"}], ":=", 
   
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "$i", ",", "$j", ",", "$n", ",", "$sol", ",", "$sol0", ",", "$sol1"}], 
      "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"$n", "=", 
       RowBox[{"Length", "[", "m", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"$sol", "[", "0", "]"}], "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"c", "[", 
          RowBox[{"$j", ",", "0"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"$j", ",", "1", ",", "$n"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"$i", "=", "1"}], ",", 
        RowBox[{"$i", "\[LessEqual]", "n"}], ",", 
        RowBox[{"$i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"$sol0", "=", 
